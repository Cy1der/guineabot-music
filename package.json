{
	"name": "guineabot-music",
	"version": "1.0.0",
	"description": "",
	"main": "./src/index.ts",
	"scripts": {
		"test": "ts-node-dev --respawn --transpile-only --poll ./src/index.ts",
		"lint": "prettier --write .",
		"build": "tsc"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Cy1der/guineabot-music.git"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/Cy1der/guineabot-music/issues"
	},
	"homepage": "https://github.com/Cy1der/guineabot-music#readme",
	"devDependencies": {
		"@commitlint/cli": "^16.2.3",
		"@commitlint/config-angular": "^16.2.3",
		"@types/glob": "^7",
		"@types/ms": "^0.7.31",
		"@types/node": "^17.0.23",
		"husky": "^7.0.4",
		"lint-staged": "^12.3.7",
		"prettier": "^2.6.2",
		"prisma": "^3.12.0",
		"ts-node": "^10.7.0",
		"ts-node-dev": "^1.1.8",
		"typescript": "^4.6.3"
	},
	"husky": {
		"hooks": {
			"pre-commit": "yarn dlx @yarnpkg/doctor && lint-staged",
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"lint-staged": {
		"*.ts": "prettier --write",
		"*.js": "prettier --write"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-angular"
		],
		"rules": {
			"scope-case": [
				2,
				"always",
				"pascal-case"
			],
			"type-enum": [
				2,
				"always",
				[
					"chore",
					"build",
					"ci",
					"docs",
					"feat",
					"fix",
					"perf",
					"refactor",
					"revert",
					"style",
					"test"
				]
			]
		}
	},
	"dependencies": {
		"@prisma/client": "^3.12.0",
		"consola": "^2.15.3",
		"discord.js": "^13.6.0",
		"erela.js": "^2.3.3",
		"erela.js-deezer": "^1.0.7",
		"erela.js-spotify": "^1.2.0",
		"glob": "^7.2.0",
		"ms": "^2.1.3"
	}
}
